{
  "name": "blueprints-full",
  "runtime": "node:22-alpine",
  "resources": [
    "arn::dynamodb:::table/blueprints-full",
    "arn::s3:::blueprints-full"
  ],
  "routes": {
    "dynamodb.amazonaws.com": "/api/dynamodb",
    "s3.amazonaws.com": "/api/s3"
  },
  "services": [
    {
      "name": "webapp",
      "src": "webapp",
      "handler": "localhost:3000",
      "runtime": "node:22-alpine",
      "files": ["node_modules", "package.json", ".next", "public"],
      "scripts": {
        "install": "npm install",
        "dev": "next dev",
        "build": "next build",
        "start": "next start"
      }
    }
  ]
}
