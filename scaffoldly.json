{
  "name": "python-open-webui",
  "runtime": "python:3.11",
  "handler": "localhost:8080",
  "files": ["app.py"],
  "packages": ["libsndfile1-dev", "pip:requirements.txt"],
  "resources": ["arn::elasticfilesystem:::file-system/.cache#XDG_CACHE_HOME"],
  "schedules": {
    "@immediately": "mount"
  },
  "scripts": {
    "start": "GLOBAL_LOG_LEVEL=DEBUG DATA_DIR=/tmp open-webui serve"
  },
  "memorySize": 1024
}
